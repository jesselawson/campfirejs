WHITESPACE = _{ " " }
COMMENT = _{ "/*" ~ (!"*/" ~ ANY)* ~ "*/" }
variable_name = { (!"$" ~ !"\n" ~ !SPACE_SEPARATOR ~ ANY)* }
begin_keyword = _{ "$begin" }
end_keyword = _{ "$end" }
card_start = _{ begin_keyword ~ SPACE_SEPARATOR* ~ variable_name ~ SPACE_SEPARATOR* ~ "\n"? }
card_body = {  (!"$end" ~ !SPACE_SEPARATOR ~ ANY)* } 
card = { card_start ~ card_body ~ end_keyword }
campfire_file = _{ SOI ~ "\n"* ~ (card ~ "\n"+) * ~ card? ~ EOI }
